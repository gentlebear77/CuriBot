<robot name="cube">
  <link name="cube_link">
    <inertial>
      <!-- Mass and inertia properties of the cube -->
      <mass value="0.08" />
      <!-- Center of mass at the origin of the link -->
      <origin xyz="0 0 0" />
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001" />
    </inertial>
    
    <visual>
      <!-- Visual representation of the cube -->
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.05 0.05 0.05"/> <!-- Cube dimensions-->
      </geometry>
      <material name="blue">
        <color rgba="0 0 255 1"/>
      </material>
    </visual>
    
    <collision>
      <!-- Collision properties of the cube -->
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.05 0.05 0.05"/> <!-- Cube dimensions-->
      </geometry>
    </collision>
  </link>
</robot>
